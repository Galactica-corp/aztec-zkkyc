{
  "name": "aztec-web-boilerplate",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist src/target src/artifacts/*.json src/artifacts/*.js src/artifacts/*.d.ts contracts/*/target",
    "build-contracts": "npx tsx scripts/build-contracts.ts --force",
    "deploy-contracts": "tsx scripts/deploy.ts --network sandbox",
    "dev": "vite",
    "build-app": "NODE_OPTIONS='--max-old-space-size=14336' vite build",
    "build-app:ci": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=6656' vite build",
    "build": "yarn build-contracts && yarn build-app",
    "build:ci": "yarn build-contracts && yarn build-app:ci",
    "serve": "vite preview --port ${PORT:-3000}",
    "prep-test": "PROVER_ENABLED=false yarn deploy-contracts && yarn build-app",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "yarn prep-test && yarn playwright test --project=chromium",
    "lint": "eslint src && prettier --check ./src",
    "lint:fix": "eslint src --fix && prettier --write ./src"
  },
  "dependencies": {
    "@azguardwallet/client": "0.7.0",
    "@azguardwallet/types": "0.7.0",
    "@aztec/accounts": "4.0.0-devnet.2-patch.0",
    "@aztec/aztec.js": "4.0.0-devnet.2-patch.0",
    "@aztec/constants": "4.0.0-devnet.2-patch.0",
    "@aztec/foundation": "4.0.0-devnet.2-patch.0",
    "@aztec/kv-store": "4.0.0-devnet.2-patch.0",
    "@aztec/noir-contracts.js": "4.0.0-devnet.2-patch.0",
    "@aztec/pxe": "4.0.0-devnet.2-patch.0",
    "@aztec/stdlib": "4.0.0-devnet.2-patch.0",
    "@aztec/wallet-sdk": "4.0.0-devnet.2-patch.0",
    "@defi-wonderland/aztec-fee-payment": "github:defi-wonderland/aztec-fee-payment#dev",
    "@defi-wonderland/aztec-standards": "0.0.0-11cd7319",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toast": "1.2.15",
    "@radix-ui/react-toggle": "1.1.10",
    "@radix-ui/react-tooltip": "1.2.8",
    "@tailwindcss/postcss": "4.1.18",
    "@tanstack/react-query": "5.85.5",
    "autoprefixer": "10.4.23",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "iso-3166": "^4.4.0",
    "lucide-react": "0.562.0",
    "postcss": "8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router-dom": "7.12.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "viem": "2.41.2",
    "zustand": "5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@playwright/test": "1.57.0",
    "@types/node": "22.15.17",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "5.0.1",
    "@vitest/ui": "3.2.4",
    "@wonderland/walletless": "1.0.0",
    "assert": "2.1.0",
    "buffer": "6.0.3",
    "crypto-browserify": "3.12.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "eslint-plugin-unused-imports": "4.3.0",
    "globals": "17.0.0",
    "jsdom": "27.3.0",
    "prettier": "3.5.3",
    "process": "0.11.10",
    "stream-browserify": "3.0.0",
    "tsx": "4.21.0",
    "typescript": "5.3.3",
    "typescript-eslint": "8.52.0",
    "util": "0.12.5",
